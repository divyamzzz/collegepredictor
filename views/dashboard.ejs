<%-include('partials/header')-%>
<div class="details">
  <div class="row">
  <div class="col-lg-6">
    <div class="info">
      <h1 style="margin: 0 0 10px 20px;">USER DETAILS</h1>      
          <ul type="none">
          <% sdetails.forEach(function(student){ %>
         <li>
          <h4>Name: <%=student.name%></h4>
        </li>
        <li>
          <h4>Email: <%=student.email%></h4>
        </li>
        <li>
          <h4>Gender: <%=student.gender%></h4>
        </li>
        <li>
          <h4>userName: <%=student.username%></h4>
        </li>
        <li>
          <h4>Category: <%=student.category%></h4>
        </li>
        <li>
          <h4>Rank: <%=student.rank%></h4>
        </li>
        <!-- <li>
          <h4>password: <%=student.password%></h4>
        </li> -->
    
      <form action="/dashboard" method="post">
        <label for="sbutton"><h6>CLICK GO TO VIEW RESULT</h6></label>
        <button type="submit" name="sbutton" value="<%=student.rank%>">GO</button>
        </form>   
      </ul>
      <% }) %>
    </div>  
  </div>
  </div>  
</div>
   <%- include('partials/footer') -%>
